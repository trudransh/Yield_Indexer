{
  "name": "yield-squid",
  "version": "1.0.0",
  "description": "Subsquid indexer for yield data on Arbitrum",
  "private": true,
  "scripts": {
    "build": "rm -rf lib && tsc",
    "codegen": "squid-typeorm-codegen",
    "typegen": "squid-evm-typegen src/abi --clean",
    "process": "node --require=dotenv/config lib/main.js",
    "process:prod": "node lib/main.js",
    "serve": "squid-graphql-server",
    "serve:prod": "squid-graphql-server",
    "migration:generate": "squid-typeorm-migration generate",
    "migration:apply": "squid-typeorm-migration apply",
    "db:reset": "squid-typeorm-migration clean"
  },
  "dependencies": {
    "@subsquid/archive-registry": "^3.3.0",
    "@subsquid/evm-processor": "^1.19.0",
    "@subsquid/graphql-server": "^4.6.0",
    "@subsquid/ss58": "^2.0.2",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.5.1",
    "dotenv": "^16.4.5",
    "ethers": "^6.16.0",
    "pg": "^8.12.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "^4.0.0",
    "@subsquid/typeorm-codegen": "^2.0.2",
    "@types/node": "^22.9.0",
    "typescript": "~5.5.0"
  }
}
